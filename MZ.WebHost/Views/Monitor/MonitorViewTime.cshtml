@{ 
    Layout = "";
    var viewList = ViewData["viewList"] as List<BsonDocument>;
    var index = 0;
}
<table class="table table-hover" width="100%">
    <thead>
        <tr>
            <th>序号</th>
            <th>地址</th>
            <th>响应时间</th>
        </tr>
    </thead>
    @foreach (var view in viewList)
    {
        index++;
        <tr>
            <td>@index</td>
            <td>
                @view.String("path")
            </td>
            <td>
                @view.String("ationRunTime") ms
            </td>
        </tr>
    }
</table>

