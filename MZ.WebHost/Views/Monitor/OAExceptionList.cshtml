
@{
    Layout = "";
    var exceptionList = ViewData["exceptionList"] as List<BsonDocument>;
}
<table class="table table-hover">

    <thead>
        <tr>
            <th>标题</th>
            <th>人员</th>
            <th>错误信息</th>
            <th>类型</th>
            <th>时间</th>
            <th>处理地址</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var except in exceptionList)
        {
            <tr>
                <td>@except.Text("title")</td>
                <td>@except.Text("userNames")</td>
                <td>@except.Text("message")</td>
                <td>@except.Text("type")</td>
                <td>@except.Text("date")</td>
                <td>@except.Text("url")</td>
            </tr>
        }
        @if (exceptionList.Count == 0)
        {
            <tr>
                <td colspan="6">暂无数据</td>
            </tr>
        }
    </tbody>
</table>
